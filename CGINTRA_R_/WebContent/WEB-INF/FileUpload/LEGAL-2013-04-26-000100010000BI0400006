

SELECT * FROM CLAIM_DETAIL  WHERE clm_status='AP' AND TRUNC(CLM_APPROVED_DT) BETWEEN TO_DATE('25/02/2013','dd/mm/yyyy') AND TO_DATE('01/03/2013','dd/mm/yyyy')




SELECT * FROM CLAIM_DETAIL_TEMP  WHERE clm_status='FW' AND TRUNC(CLM_CREATED_MODIFIED_DT) BETWEEN TO_DATE('25/02/2013','dd/mm/yyyy') AND TO_DATE('01/03/2013','dd/mm/yyyy')