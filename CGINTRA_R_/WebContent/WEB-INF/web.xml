  <!-- Struts Tag Library Descriptors -->
  <taglib>
    <taglib-uri>/WEB-INF/TLD/struts-bean.tld</taglib-uri>
    <taglib-location>/WEB-INF/TLD/struts-bean.tld</taglib-location>
  </taglib>

  <taglib>
    <taglib-uri>/WEB-INF/TLD/struts-html.tld</taglib-uri>
